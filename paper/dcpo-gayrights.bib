	journal = {Sociological Forum},
	keywords = {democratization, education, homosexuality, modernization, political freedom, tolerance},
	number = {2},
	pages = {501-521},
	title = {Tolerance of Homosexuality in 88 Countries: Education, Political Freedom, and Liberalism},
